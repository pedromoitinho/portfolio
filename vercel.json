{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/static/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "images": {
      "domains": ["raw.githubusercontent.com"],
      "sizes": [640, 750, 828, 1080, 1200],
      "minimumCacheTTL": 60
    },
    "routes": [
      {
        "src": "/static/(.*)",
        "headers": { "cache-control": "public,max-age=31536000,immutable" }
      }
    ]
  }